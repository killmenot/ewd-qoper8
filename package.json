{
  "name": "ewd-qoper8",
  "version": "3.13.0",
  "description": "High-performance Message Queue for Node.js",
  "main": "index.js",
  "scripts": {
    "lint": "jshint spec",
    "test": "npm run test:unit",
    "test:unit": "jasmine --config=spec/support/unit.json",
    "test:integration": "jasmine --config=spec/support/integration.json",
    "coverage:clean": "rm -rf ./coverage",
    "coverage:unit": "istanbul cover jasmine -- --config=spec/support/unit.json",
    "coverage:integration": "istanbul cover jasmine -- --config=spec/support/integration.json",
    "coveralls:unit": "npm run coverage:unit && coveralls < coverage/lcov.info",
    "coveralls:integration": "npm run coverage:integration && coveralls < coverage/lcov.info",
    "coveralls": "npm run coverage:clean && npm run coveralls:unit && npm run coveralls:integration"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robtweed/ewd-qoper8.git"
  },
  "author": "Rob Tweed, M/Gateway Developments Ltd",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/robtweed/ewd-qoper8/issues"
  },
  "homepage": "https://github.com/robtweed/ewd-qoper8#readme",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "istanbul": "^0.4.5",
    "jasmine": "^2.7.0",
    "jasmine-spec-reporter": "^4.1.1",
    "jshint": "^2.9.5",
    "pre-commit": "^1.2.2",
    "rewire": "^2.5.2"
  },
  "pre-commit": [
    "lint",
    "test:unit"
  ]
}
